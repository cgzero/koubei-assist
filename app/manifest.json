{
    "name": "百度口碑助手",
    "manifest_version": 2,
    "version": "2.0.6",
    "description": " 百度口碑助手",
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "icons": {
        "16": "src/img/icon19.png",
        "48": "src/img/icon48.png",
        "128": "src/img/icon128.png"
    },
    "permissions":["tabs", "webRequest", "webRequestBlocking", "<all_urls>"],
    "browser_action": {
        "default_icon": {
            "19": "src/img/icon19.png",
            "38": "src/img/icon38.png"
        },
        "default_title": "百度口碑助手",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["src/background.js", "dep/jquery/jquery-2.2.4.min.js"]
    }
}
